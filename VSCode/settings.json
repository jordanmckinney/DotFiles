{
  "workbench.iconTheme": "material-icon-theme",
  "explorer.sortOrder": "type",
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.tabSizing": "shrink",
  "workbench.colorTheme": "Material Theme Palenight High Contrast",
  "editor.fontFamily": "Fira Code, Menlo, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.fontWeight": "500",
  "editor.formatOnSave": true,
  "editor.minimap.enabled": false,
  "editor.insertSpaces": true,
  "editor.hover.delay": 500,
  "editor.lightbulb.enabled": false,
  "editor.renderLineHighlight": "gutter",
  "editor.smoothScrolling": true,
  "editor.parameterHints.enabled": false,
  "editor.suggest.snippetsPreventQuickSuggestions": false,
  "html.suggest.html5": false,
  "editor.parameterHints": false,
  "editor.tabCompletion": "on",
  "editor.cursorBlinking": "smooth",
  "editor.suggest.localityBonus": true,
  "editor.suggest.shareSuggestSelections": true,
  "editor.suggestFontSize": 11,
  "workbench.commandPalette.history": 10,
  "workbench.useExperimentalGridLayout": true,
  "workbench.statusBar.feedback.visible": false,
  "breadcrumbs.filePath": "off",
  "workbench.editor.closeOnFileDelete": true,
  "terminal.integrated.fontWeight": "500",
  "bracket-pair-colorizer-2.showHorizontalScopeLine": false,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "bracket-pair-colorizer-2.colors": ["#87a5ff", "#b087ff", "#f187ff", "#ff8787", "#87ffd3"],
  "javascript.updateImportsOnFileMove.enabled": "always",
  "git.autofetch": true,
  "workbench.startupEditor": "newUntitledFile",
  "python.pythonPath": "/usr/local/bin/python3",
  "python.linting.enabled": true,
  "python.linting.lintOnSave": true,
  "python.linting.pylintEnabled": true,
  "python.linting.pylintPath": "/usr/local/bin/pylint",
  "python.formatting.autopep8Path": "/usr/local/bin/autopep8",
  "python.formatting.autopep8Args": ["--aggressive", "--aggressive"],
  "editor.wordWrap": "on",
  "files.trimFinalNewlines": true,
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 5000,
  "files.exclude": {
    "node_modules": true
  },
  "[javascript]": {
    "editor.tabSize": 2
  },
  "[typescript]": {
    "editor.tabSize": 2
  },
  "[html]": {
    "editor.tabSize": 2
  },
  "[css]": {
    "editor.tabSize": 2
  },
  "[scss]": {
    "editor.tabSize": 2
  },
  "vim.leader": "<space>",
  "vim.insertModeKeyBindings": [
    {
      "before": ["k", "k"],
      "after": ["<esc>"]
    },
    {
      "before": ["j", "j"],
      "after": ["<esc>"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [";"],
      "commands": ["workbench.action.showCommands"]
    },
    {
      "before": ["leader", "w"],
      "commands": ["workbench.action.files.save"]
    },
    {
      "before": ["j"],
      "after": ["g", "j"]
    },
    {
      "before": ["k"],
      "after": ["g", "k"]
    }
  ],
  "explorer.confirmDragAndDrop": false,
  "explorer.openEditors.visible": 5,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "terminal.integrated.rendererType": "dom",
  "gitlens.views.repositories.location": "gitlens",
  "gitlens.views.fileHistory.location": "gitlens",
  "gitlens.views.lineHistory.location": "gitlens",
  "gitlens.views.compare.location": "gitlens",
  "gitlens.views.search.location": "gitlens",
  "gitlens.currentLine.enabled": false,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.blame.heatmap.enabled": false,
  "gitlens.blame.highlight.enabled": false,
  "gitlens.hovers.enabled": false,
  "gitlens.codeLens.enabled": false,
  "gitlens.statusBar.enabled": false,
  "gitlens.views.repositories.enabled": false,
  "gitlens.views.search.enabled": false,
  "gitlens.mode.statusBar.enabled": false,
  "nativescript.analytics.enabled": false,
  "explorer.confirmDelete": false,
  "python.jediEnabled": false
}
